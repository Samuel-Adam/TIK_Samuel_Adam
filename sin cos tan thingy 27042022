#include <iostream>
#include <math.h>
using namespace std;

int main(){
    float a = sqrt(15);//menghitung akar kuadrat
    float b = cbrt(27);//menghuitung akar kubik
    float c = pow(4.334,2.231);//menghitung pangkat
    float d = sin(15);
    float e = cos(15);
    float f = tan(15);
    
    cout<<a<<endl;
cout<<b<<endl;
cout<<c<<endl;
cout<<d<<endl;
cout<<e<<endl;
cout<<f<<endl;
    return 0;
    
    
}
